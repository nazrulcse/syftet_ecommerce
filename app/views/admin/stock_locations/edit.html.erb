<% content_for :page_title do %>
    <%= link_to t(:stock_locations), admin_stock_locations_url %> /
    <%= @stock_location.name %>
<% end %>

<%= render :partial => 'admin/shared/error_messages', :locals => {:target => @stock_location} %>

<%= form_for [:admin, @stock_location] do |f| %>
    <fieldset>
      <%= render :partial => 'form', :locals => {:f => f} %>
      <%= render :partial => 'admin/shared/edit_resource_links' %>
    </fieldset>
<% end %>
