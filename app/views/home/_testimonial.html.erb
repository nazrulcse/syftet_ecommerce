<section class="client-testimonial row">
  <div class="kc-row-container  kc-container">
    <div class="kc-wrap-columns">
      <div class="col-sm-7">
        <div class="kc-col-container">

          <div class="kc-title-wrap ">
            <h3 class="public-title"><%= t(:what_say_people) %></h3>
          </div>
          <div class="testimonial-slider">
            <!-- Wrapper for slides -->

            <% @feedbacks.each do |feedback| %>
            <%# feedback = @feedbacks.last %>
                <div class="owl-item active first">
                  <div class="item">
                    <div class="testimonials">
                      <div class="testimonials-body">
                        <div class="testimonials-avatar list-unstyled">
                          <div class="avatar">

                            <%= image_tag 'male-avater.png', class: 'img-responsive' %>
                            <!--<img width="100" height="100" src="http://venusdemo.com/wordpress/pi/wp-content/uploads/2014/06/u-100x100.jpg" class="attachment-pi-avatar size-pi-avatar wp-post-image" alt="">-->
                          </div>
                        </div>
                        <div class="testimonials-info">
                          <h5 class="testimonials-name">
                            <%= feedback.name %>
                          </h5>
                          <p>
                            <label> Ratings: </label>
                            <%= feedback.rate %>
                          </p>

                          <!--<p class="text-muted testimonials-position">-->
                            <!--<a href="#">-->
                              <!--<%#= feedback.email %>-->
                            <!--</a>-->
                          <!--</p>-->
                        </div>
                        <p class="testimonials-description">
                          <%= feedback.message %>
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
            <% end %>

          </div>

        </div>
      </div>

      <div class="col-sm-5">
        <div class="kc-col-container">
          <div class=" kc_ins_col_3">
            <ul>
              <li class="el-start loaded">
                <a href="" class="zoom-img">
                  <img class="zoomimg" src="https://scontent.cdninstagram.com/t51.2885-15/s150x150/e35/14487425_599839940222639_2169011080649506816_n.jpg">
                </a>
              </li>
              <li class=" loaded">
                <a href="" target="_blank">
                  <img src="https://scontent.cdninstagram.com/t51.2885-15/s150x150/e35/14547700_1078505402269165_7299002542493532160_n.jpg">
                </a>
              </li>
              <li class="el-end loaded">
                <a href="" target="_blank">
                  <img src="https://scontent.cdninstagram.com/t51.2885-15/s150x150/e35/14448338_190048458086402_2638088682661216256_n.jpg">
                </a>
              </li>
              <li class="el-start loaded">
                <a href="" target="_blank" class="zoom-img"><img src="https://scontent.cdninstagram.com/t51.2885-15/s150x150/e35/14540628_298862940494930_209775376786784256_n.jpg"></a>
              </li>
              <li class=" loaded">
                <a href="" target="_blank"><img src="https://scontent.cdninstagram.com/t51.2885-15/s150x150/e35/14488275_603033969865155_4489113900976963584_n.jpg"></a>
              </li>
              <li class="el-end loaded">
                <a href="" target="_blank"><img src="https://scontent.cdninstagram.com/t51.2885-15/s150x150/e35/14533548_709490632538083_2315626691310911488_n.jpg"></a>
              </li>
            </ul>
          </div>
          <div class="">
            <p>
             <strong>Instagram</strong>:&nbsp;<a href="https://www.instagram.com/p/BK-LKGej-fD/">#Pibags</a>,<a href="https://www.instagram.com/p/BK-LLa3j6YN/">#minimal</a>,<a href="https://www.instagram.com/p/BK-LI0vjBI3/">#bags</a>
            </p>

            <div id="SL_balloon_obj" style="display: block;">
              <div id="SL_button" class="SL_ImTranslatorLogo" style="background: url('chrome-extension://noaijdpnepcgjemiklgfkcfbkokogabh/content/img/util/imtranslator-s.png'); display: none; opacity: 1;"></div>
              <div id="SL_shadow_translation_result2" style="display: none;"></div>
              <div id="SL_shadow_translator" style="display: none;">
                <div id="SL_planshet" style="background: url('chrome-extension://noaijdpnepcgjemiklgfkcfbkokogabh/content/img/util/bg2.png') #f4f5f5;">
                  <div id="SL_arrow_up" style="background: url('chrome-extension://noaijdpnepcgjemiklgfkcfbkokogabh/content/img/util/up.png');"></div>
                  <div id="SL_providers">
                    <div id="SL_P0" class="SL_BL_LABLE_ON" title="Google">G</div>
                    <div id="SL_P1" class="SL_BL_LABLE_ON" title="Microsoft">M</div>
                    <div id="SL_P2" class="SL_BL_LABLE_ON" title="Translator">T</div>
                  </div>
                  <div id="SL_alert_bbl" style="display: none;"></div>
                  <div id="SL_TB">
