<!DOCTYPE html>
<html>
<head>
  <title>Lienesbeauty | UK Product</title>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.png' %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => 'reload' %>
  <%= render 'shared/header' %>
</head>

<body>
<div id="page">
  <%= render 'shared/notification' %>
  <%= render 'shared/custom_flash' %>
  <div class="syftet-page-inner row-offcanvas row-offcanvas-left">
    <div class="top-nav">
      <div class="container">
        <%= render 'shared/top_nav' %>
      </div>
    </div>
    <%= render 'shared/main_menu' %>
    <%= yield :breadcrumb %>
    <div class="container">
      <%= yield :sidebar %>
      <%= yield %>
    </div>
    <%= render 'products/compare_modal' %>
    <footer id="syftet-footer" class="syftet-footer" role="contentinfo">
      <%= render 'shared/footer' %>
      <%= render 'shared/public_footer' %>
    </footer>
  </div>
</div>
</body>
</html>